{
    "manifest_version": 2,
    "name": "Potato Launcher",
    "description": "19-1 New Tab Extension",
    "version": "1.0.0",
    "icons": { "128": "icon_10.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "/popup/popup.html"
    },
    "chrome_url_overrides" : {
        "newtab": "/newtab/newtab.html"
    },
    "permissions": ["activeTab"]
}